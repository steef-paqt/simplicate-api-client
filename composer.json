{
    "name": "paqtcom/simplicate-api-client",
    "description": "Simplicate API client",
    "require-dev": {
        "phpmd/phpmd": "^2.15",
        "rector/rector": "*",
        "squizlabs/php_codesniffer": "^3.9",
        "friendsofphp/php-cs-fixer": "^3.64",
        "phpstan/phpstan": "^1.12"
    },
    "autoload": {
        "psr-4": {
            "Paqtcom\\Simplicate\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Steef de Winter",
            "email": "steef.dewinter@paqt.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "jane-php/json-schema-runtime": "^7.8",
        "psr/http-client": "^1.0",
        "jane-php/open-api-runtime": "^7.8",
        "symfony/validator": "^6.4.0"
    },
    "config": {
        "allow-plugins": {
            "php-http/discovery": false
        }
    }
}
